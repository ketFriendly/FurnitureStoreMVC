@model IEnumerable<FurnitureStore.Models.Store>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/>
<h4>Choose a store you wish to browse </h4>
<br />
<div align="left">
    @using (Html.BeginForm("ReadOnlyList", "Browsing", FormMethod.Get))
    {
        
        @Html.DropDownList("StoreID", null, htmlAttributes: new { @class = "form-control" })
        <br/>
        <input  class="btn btn-info" type="submit" name="submit" value="Search" />
    }
</div>